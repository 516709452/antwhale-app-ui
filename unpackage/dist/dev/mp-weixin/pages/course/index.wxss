/* 略缩图 */
.uni-thumb {
  flex-shrink: 0;
  margin-right: 20rpx;
  width: 125px;
  height: 75px;
  border-radius: 12rpx;
  overflow: hidden;
  border: 1px #f5f5f5 solid;
}
.uni-thumb image {
  width: 100%;
  height: 100%;
}
.slot-box {
  display: flex;
  flex-direction: row;
  align-items: center;
}
.slot-text {
  flex: 1;
  font-size: 12px;
}

